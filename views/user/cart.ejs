                        <form action="#">               
                            <div class="table-content wnro__table table-responsive">
                                <table>
                                    <thead>
                                        <tr class="title-top">
                                            <th class="product-thumbnail">Image</th>
                                            <th class="product-name">Product</th>
                                            <th class="product-price">Price</th>
                                            <th class="product-quantity">Quantity</th>
                                            <th class="product-subtotal">Total</th>
                                            <th class="product-remove">Remove</th>
                                        </tr>
                                    </thead>
                                    <tbody data-bind="foreach:details">
                                        <tr>
                                            <td class="product-thumbnail"> <a href="#"><img data-bind="attr:{src: image,alt:image}"></a></td>
                                           
                                            <td class="product-name"><span id="productid" data-bind="text:$data.productName"></span></td>
                                            <!-- <a href="#">Natoque penatibus</a> -->
                                            <td class="product-price"><span class="amount" data-bind="text:$data.price"></span></td>
                                            <td class="product-quantity"><input type="number" id="qt" data-bind="value:$data.quantity"></td>
                                            <td class="product-subtotal"><span data-bind="text:$data.amount"></span></td>
                                            <td class="product-remove"><a href="#" data-bind="click:$parent.removeFromCart">X</a></td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </form> 
                        <div class="cartbox__btn" >
                            <ul class="cart__btn__list d-flex flex-wrap flex-md-nowrap flex-lg-nowrap justify-content-between">
